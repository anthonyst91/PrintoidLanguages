    <!-- VERSIONS COMPARATOR -->
    <string name="tutorial_comparator_title">版本比较</string>
    <string name="tutorial_comparator_premium_title" translatable="false">Printoid旗舰版</string>
    <string name="tutorial_comparator_premium_content">视频流监控, 自定义GCODE指令, 2D及3D查看器, 实时模拟器, 高级控制, 桌面部件等更多…</string>
    <string name="tutorial_comparator_lite_title" translatable="false">Printoid精简版</string>
    <string name="tutorial_comparator_lite_content">包含免费的基础功能, 可永久使用的</string>
    <string name="tutorial_comparator_button_1">比较版本</string>

    <!-- WIDGETS -->
    <string name="tutorial_widgets_title">独有的桌面部件</string>
    <string name="tutorial_widgets_message">不用打开应用就可监控您的3D打印机!</string>
